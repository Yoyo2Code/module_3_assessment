<p>
  <%= @store_count %> Total Stores
</p>

<table>
  <tr>
    <th>Long Name</th>
    <th>City</th>
    <th>Distance</th>
    <th>Phone Number</th>
    <th>Store Type</th>
  </tr>
  <% @stores.each do |store| %>
  <tr class="store-details">
    <td><%= store.long_name %> </td>
    <td><%= store.city %> </td>
    <td><%= store.distance %> </td>
    <td><%= store.phone_number %> </td>
    <td><%= store.store_type %> </td>
  </tr>
  <% end %>
</table>
